# Unleash Feature Flags Configuration
REACT_APP_UNLEASH_PROXY_URL=http://localhost:3002/proxy
REACT_APP_UNLEASH_CLIENT_KEY=default:development.unleash-insecure-frontend-api-token
REACT_APP_UNLEASH_APP_NAME=sprkz-pdf-forms
REACT_APP_UNLEASH_ENVIRONMENT=development

# PDF Processing
REACT_APP_PDF_WORKER_URL=/pdf.worker.min.mjs

# Sentry Configuration (for error reporting feature flag)
REACT_APP_SENTRY_DSN=your-sentry-dsn-here
REACT_APP_SENTRY_ENVIRONMENT=development

# Development Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_PERFORMANCE_MONITORING=true

# API Configuration (future use)
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=30000

# Feature Flag Default Overrides (optional)
# Set these to override default feature flag values when Unleash is unavailable
REACT_APP_DEFAULT_ENHANCED_WIZARD_MODE=true
REACT_APP_DEFAULT_PROGRESSIVE_FORM_FILLING=true
REACT_APP_DEFAULT_SIGNATURE_DRAWING_MODE=true
REACT_APP_DEFAULT_SIGNATURE_TYPED_MODE=true
REACT_APP_DEFAULT_ANIMATION_EFFECTS=true
REACT_APP_DEFAULT_PDF_LAZY_LOADING=true